{
    "css": {
        "allowed_file_extensions": [
            "css",
            "scss",
            "sass",
            "less"
        ],
        "end_with_newline": false,
        "indent_char": " ",
        "indent_size": 4,
        "newline_between_rules": true,
        "selector_separator": " ",
        "selector_separator_newline": false,
        "preserve_newlines": false,
        "max_preserve_newlines": 1
    }
}